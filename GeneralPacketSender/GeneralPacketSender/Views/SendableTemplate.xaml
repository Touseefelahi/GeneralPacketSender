<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewmodel="clr-namespace:GeneralPacketSender.Viewmodels">
    <DataTemplate DataType="{x:Type viewmodel:SenderViewmodel}">
        <Grid VerticalAlignment="Top" HorizontalAlignment="Left" Margin="5">
            <StackPanel>
                <ContentPresenter Content="{Binding Sendable}"/>
                <Button Content="Send" Command="{Binding SendCommand}"/>
            </StackPanel>
        </Grid>
    </DataTemplate>
</ResourceDictionary>